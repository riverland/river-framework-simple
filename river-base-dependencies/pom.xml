	<profiles>
		<profile>
			<id>oracle</id>
			<properties>
				<db.name>oracle</db.name>
				<jdbc.driver.groupId>com.oracle</jdbc.driver.groupId>
				<jdbc.driver.artifactId>ojdbc14</jdbc.driver.artifactId>
				<jdbc.driver.version>10.2.0.2.0</jdbc.driver.version>
				<jdbc.driverClassName>oracle.jdbc.OracleDriver</jdbc.driverClassName>
				<jdbc.url>
				<![CDATA[]]>
				</jdbc.url>
				<jdbc.username></jdbc.username>
				<jdbc.password></jdbc.password>
				<hibernate.dialect>org.hibernate.dialect.Oracle10gDialect</hibernate.dialect>
			</properties>
		</profile>

		<profile>
			<id>sqlserver</id>
			<properties>
				<db.name>sqlserver</db.name>
				<jdbc.driver.groupId>com.microsoft</jdbc.driver.groupId>
				<jdbc.driver.artifactId>sqlserver</jdbc.driver.artifactId>
				<jdbc.driver.version>2005</jdbc.driver.version>
				<jdbc.driverClassName>com.microsoft.jdbc.SQLServerDriver</jdbc.driverClassName>
				<jdbc.url>
				   <![CDATA[]]>
				</jdbc.url>
				<jdbc.username></jdbc.username>
				<jdbc.password></jdbc.password>
				<hibernate.dialect>org.hibernate.dialect.SQLServerDialect</hibernate.dialect>
			</properties>
		</profile>

		<profile>
			<id>db2</id>
			<properties>
				<db.name>db2</db.name>
				<jdbc.driver.groupId>com.ibm</jdbc.driver.groupId>
				<jdbc.driver.artifactId>db2jcc</jdbc.driver.artifactId>
				<jdbc.driver.version>2005</jdbc.driver.version>
				<jdbc.driverClassName>com.microsoft.jdbc.DB2Driver</jdbc.driverClassName>
				<jdbc.url>
				   <![CDATA[]]>
				</jdbc.url>
				<jdbc.username></jdbc.username>
				<jdbc.password></jdbc.password>
				<hibernate.dialect>org.hibernate.dialect.DB2Dialect</hibernate.dialect>
			</properties>
		</profile>
